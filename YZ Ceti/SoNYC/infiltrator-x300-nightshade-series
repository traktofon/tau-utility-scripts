        <script type="text/javascript">
    /* Set up event hooks */
    (function (Veure) {

        // internal state
        var V = {
            // Current registered modules
            registeredModules : {},
            // Register a module, notify observers
            registerModule: function (name, module) {
                Veure.debug(Veure.MODULE.INIT, 'registered module ' + name);
                // force a true value to ensure registration checks succeed
                module = module || true;
                V.registeredModules[name] = module;
                var observers = (V.moduleRegistrationObservers[name] || []);
                for (var i = 0, j = observers.length; i < j; i++) {
                    Veure.debug(Veure.MODULE.INIT, 'firing observer callback for module ' + name);
                    observers[i](module, name);
                }
                delete V.moduleRegistrationObservers[name];
            },
            // Observers to be notified when a module registers
            moduleRegistrationObservers: {},
            // Register a module registration observer
            onModuleRegistration: function (name, callback) {
                Veure.debug(Veure.MODULE.INIT, 'registering callback for module ' + name);
                V.moduleRegistrationObservers[name] = (V.moduleRegistrationObservers[name] || []);
                V.moduleRegistrationObservers[name].push(callback);
            },
            // Check whether a given module's dependency modules have all registered
            dependenciesResolved: function (dependencies, current) {
                for (var i = 0, j = dependencies.length; i < j; i++) {
                    var dependency = dependencies[i];
                    if (!current[i]) {
                        return false;
                    }
                }
                return true;
            }
        };

        window.Veure = Veure = {
            MODULE: {
                INIT:       '__INIT__',
                VOID:       '__VOID__',

                LIB_JQUERY:             'jquery',
                LIB_JQUERY_ADMIN:       'jquery_admin',
                LIB_JQUERY_UI:          'jquery_ui',
                LIB_JQUERY_DATATABLES:  'jquery_datatables',
                LIB_BACKBONE:           'backbone',
                LIB_JOINT:              'joint',
                LIB_BOOTSTRAP:          'bootstrap',
                LIB_BOOTSTRAP_TABLE:    'bootstrap_table',
                LIB_EVENTSOURCE:        'eventsource',
                LIB_LOCALFORAGE:        'localforage',
                LIB_TOOLTIP:            'lib_tooltip',

                CORE:       'core',
                KONGREGATE_REDIRECT: 'kongrergate-redirect',
                COMMON:     'common',
                GCT:        'gct',
                TIMER_GLOBAL: 'timer_global',
                CONFIRM: 'confirm',
                CONFIRM_MANAGER: 'confirm_manager',
                STATS_UI: 'stats_ui',
                TIMER_STATS: 'timer_stats',
                TIMER_ACTIVITY: 'timer_activity',
                MISSION_DIALOGUE:   'mission_dialogue',
                STATION_NAVIGATION: 'station_navigation',
                STATION_UI:         'station_ui',
                CHARACTER:  'character',
                CONTACTS:   'contacts',
                EVENT:      'event',
                FORM_SESSION_STORAGE:  'form-session-storage',
                BONDS:      'bonds',
                ADMIN:      'admin',
                CLONE_CENTER:   'clone_center',
                PREFERENCES:    'preferences',
                NOTIFICATION_TOGGLER: 'notification-toggler',
                SHUTTLE_SCHEDULE: 'shuttle-schedule',
                CHAT: 'chat',
                TABS: 'tabs',
                TABS_AREA: 'tabs_area',
                ACTION_TABLE: 'action-table',
                AREA_TOGGLER: 'area-toggler',
                SLIDER: 'slider',
                SICKBAY: 'sickbay',
                INVENTORY: 'inventory',
                INVENTORY_NEW: 'inventory-new',
                TRAVEL: 'travel',
                GYM: 'gym',
                INN: 'inn',
                UNIVERSITY: 'university',
                LOCAL_SHUTTLES: 'local-shuttles',
                SHIPPING_BAY: 'shipping-bay',
                INTERSTELLAR_SHUTTLES: 'interstellar-shuttles',
                HASH_LINKER: 'hash-linker',
                UNIVERSITY_COURSE_INNER: 'university-course-inner',
                VENDOR_ITEM: 'vendor-items',
                INVENTORY_COMMON: 'inventory-common',
                EMAIL_READ: 'email_read',
                EMAIL_WRITE: 'email-write',
                FORUM_WRITE: 'forum-write',
                BLOG_WRITE: 'blog-write',
                COMBAT: 'combat',
                MISSION_HISTORY: 'mission-history',
                STORAGE_ITEM: 'storage-items',
                ACCORDION: 'accordion',
                SIDEBAR: 'sidebar',
                DASHBOARD: 'dashboard',
                BANK: 'bank',
                CHARACTER_CREATION: 'character-creation',
                CLICK_OFF: 'click-off',
                SCROLLER: 'scroller',
                PLATFORM_DETECTION: 'platform-detection',
                ISSUE_REPORT: 'issue-report',
                POPUP: 'popup',
                LEVELUP: 'level-up',
                RATIONS_POPUP: 'rations-popup',
                SYNDICATE_CAMPAIGN_POPUP: 'syndicate-campaign-popup',
                RUINS: 'ruins',
                PUBLIC_MARKET: 'public-market',
                TOOLTIP: 'tooltip',
                AUTOCOMPLETE: 'autocomplete',
                DISMISS: 'dismiss',
                THIRD_PARTY_COOKIE_DETECT: 'third_party_cookie_detect',
                DOCKS: 'docks',
                SHIP: 'ship',
                SHIPYARD: 'shipyard',
                TOOLTIP_BASIC: 'tooltip-basic',
                GOVERNMENT_CENTER: 'government-center',
                SYNDICATES: 'syndicates',
                SYNDICATE_CREATION: 'syndicate-creation',
                CHECKBOX_SUM: 'checkbox-sum',
                HELP: 'help',
                KONGREGATE_LINK: 'kongregate-link',
                CAROUSEL: 'carousel',
                AVATAR_SELECTION: 'avatar-selection',
                SECURITY: 'security',
                SYNDICATE_REWARDS: 'syndicate-rewards',
                TIMER_SHUTTLE: 'timer-shuttle',
                SYNDICATE_UPDATE_MEMBER_NOTE: 'syndicate-update-member-note',
                TIMER_CONFINED: 'timer-confined',
                CAREER: 'career',
                CORETECH_PROTOTYPE: 'coretech',
                EMOJIS: 'emojis',
                FAQ: 'faq',
                RESOURCE_HUB_TUTORIALS: 'resource-hub-tutorials',
                AVATAR_CAROUSEL: 'avatar-carousel',
                FILTER: 'filter',
                CORETECHS_STORAGE: 'coretechs-storage',
                CORETECHS_STORAGE_TRANSIT: 'coretechs-storage-transit',
                CORETECHS_STORAGE_LOCKERS: 'coretechs-storage-lockers',
                PAGER: 'pager',
                INTRO: 'intro',
                CITIZEN_COUNCIL: 'citizen-council'
            },

            _whenReadyListeners: [],

            _whenReady: function (f) {
                Veure._whenReadyListeners.push(f);
            },

            inIframe: function () {
                try {
                    return window.self !== window.top;
                } catch (e) {
                    return true;
                }
            },

            init: function (dependencies, f) {
                Veure._whenReady(function () {
                    Veure.debug(Veure.MODULE.INIT, 'initialising with dependencies ' + dependencies.join(', '));
                    Veure.registerModule(Veure.MODULE.INIT, dependencies, f);
                });

                document.addEventListener('DOMContentLoaded', function () {
                    Veure.debug(Veure.MODULE.INIT, 'DOM has loaded, firing listeners');
                    Veure._whenReadyListeners.forEach(function (f) { f(); });
                });
            },

            runWith: function (dependencies, f) {
                Veure._whenReady(function () {
                    Veure.debug(Veure.MODULE.INIT, 'running in void context with dependencies ' + dependencies.join(', '));
                    Veure.registerModule(Veure.MODULE.VOID, dependencies, f);
                });
            },

            debug: function (module, message) {
                var veure_debug = false;
                if (!veure_debug) { return; }
                console.debug('Veure [' + module + ']: ' + message);
            },

            error: function (module, message) {
                throw new Error('Veure [' + module + ']: ' + message);
            },

            registerModule: function (name, dependencies, module) {
                Veure.debug(Veure.MODULE.INIT, 'registering module ' + name);

                if (!dependencies.length) {
                    return V.registerModule(name, module());
                }

                var runWith = [];

                var resolveDependency = function (index) {
                    var dependency = dependencies[i];

                    if (dependency === undefined) {
                        Veure.error(name, 'unknown dependency!');
                        return;
                    }

                    if (V.registeredModules[dependency]) {
                        runWith[index] = V.registeredModules[dependency];
                    } else {
                        Veure.debug(Veure.MODULE.INIT, 'setting observer callback timeout for module ' + name + ', dependency ' + dependency);
                        var timeout = window.setTimeout(function () {
                            Veure.error(Veure.MODULE.INIT, '10s timeout on module dependency registration. Module ' + name + ', dependency ' + dependency);
                        }, 10000);
                        V.onModuleRegistration(dependency, function (registeredDependency) {
                            Veure.debug(Veure.MODULE.INIT, 'clearing observer callback timeout for module ' + name + ', dependency ' + dependency);
                            window.clearTimeout(timeout);
                            runWith[index] = registeredDependency;
                            if (V.dependenciesResolved(dependencies, runWith)) {
                                V.registerModule(name, module.apply(Veure, runWith));
                            }
                        });
                    }
                };

                for (var i = 0, j = dependencies.length; i < j; i++) {
                    resolveDependency(i);
                }

                if (V.dependenciesResolved(dependencies, runWith)) {
                    V.registerModule(name, module.apply(Veure, runWith));
                }
            }
        };

        
                Veure.init(
                    [
                        Veure.MODULE.LIB_JQUERY, Veure.MODULE.GCT, Veure.MODULE.STATION_UI, Veure.MODULE.MISSION_DIALOGUE,
                        Veure.MODULE.CHAT
                    ],
                    function ($, GCT, StationUI, MissionDialogue, Chat) {
                        Veure.debug(Veure.MODULE.INIT, 'Veure has completed loading, here we go.');
                        var page_load_token = $('html').data('page-load-token');

                        var mainGCTClock = document.querySelector('#gct_display');
                        if(mainGCTClock) {
                            var seed_epoch = mainGCTClock.dataset.seedEpoch;
                            GCT.datetimeClock(mainGCTClock, seed_epoch);
                        }

                        // Add scrolling for the page content, if the game is in iframe, see #5667
                        // Hide logout button
                        if (Veure.inIframe()) {

                            var pcEl = document.querySelector('div.page-container');
                            if (pcEl) {
                                pcEl.style.height = '100vh';
                            }

                            window.addEventListener('load', function () {
                                document.querySelector('.avatar-links--item--logout').hidden = true;
                            });
                        }

                        // Some pages do not have page_load_token (e.g. '/login', '/forum', '/blog', ... )
                        // and we need to return here for those, so that we don't get a 404
                        // ( we have page_load_token only when logged in )
                        if ( !page_load_token ) { return; }

                        var call_url = '/page_loaded/' + page_load_token;
                        $.post(call_url);

                        StationUI.init();
                        MissionDialogue.init();
                        Chat.init();
                    }
                );
        
    })((window.Veure = {}));
</script>


        
        

